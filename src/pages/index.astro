import HomeValues from '../components/home/HomeValues.astro'
import HomeWorks from '../components/home/HomeWorks.astro'
import SmartSpacer from '../subcomponents/SmartSpacer.astro'
---

<PrimaryLayout>
  <SmartSpacer sizeClass = {'py-c4'}></SmartSpacer>
  <HomeTyped></HomeTyped>
  <SmartSpacer sizeClass = {'py-d2'}></SmartSpacer>
  <HomeHero></HomeHero>
  <SmartSpacer sizeClass = {'py-d3'}></SmartSpacer>
  <HomeCards></HomeCards>
  <SmartSpacer sizeClass = {'py-d10'}></SmartSpacer>
  <HomeValues></HomeValues>
  <SmartSpacer sizeClass = {'py-d10'}></SmartSpacer>
  <HomeWorks></HomeWorks>
  <SmartSpacer sizeClass = {'py-d10'}></SmartSpacer>
</PrimaryLayout>

